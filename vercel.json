{
  "version": 2,
  "builds": [
    { "src": "*.php", "use": "@vercel/php" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "https/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.php)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
